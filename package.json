{
  "name": "@lgslabs/bits",
  "version": "5.0.1",
  "description": "Bits Integrated Technology System",
  "author": "LGS Innovations",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/LGSInnovations/bits/issues"
  },
  "homepage": "https://github.com/LGSInnovations/bits#readme",
  "main": "index.js",
  "jest": {
    "verbose": true,
    "testRegex": "(/test(/.*)*/test-.*)\\.js?$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/lib/**/*.js"
    ],
    "setupFiles": [
      "<rootDir>/test/setup/main.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/support",
      "<rootDir>/test/mocks"
    ]
  },
  "scripts": {
    "build": "npm install && npm run lint && npm run test",
    "bits:install": "npm install --prod",
    "lint": "bits-eslint --ext js --fix ./",
    "test": "jest",
    "dev": "HTTP_PORT=9000 HTTPS_PORT=9001 node app.js -v",
    "watch": "ibazel run :app"
  },
  "devDependencies": {
    "@bazel/bazel": "^2.0.0",
    "@bazel/ibazel": "^0.11.1",
    "@bazel/typescript": "^1.0.1",
    "@lgslabs/bits-eslint": "^1.1.0",
    "jest": "^24.9.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@lgslabs/bits-core": "^2.0.0",
    "@lgslabs/bits-crypto": "^2.0.2",
    "@lgslabs/bits-fs": "^1.1.1",
    "@lgslabs/bits-level-kv": "^2.0.0",
    "@lgslabs/bits-logger": "^2.0.0",
    "@lgslabs/bits-memory-crud": "^2.0.0",
    "@lgslabs/bits-memory-kv": "^2.0.0",
    "@lgslabs/bits-message-center": "^1.0.4",
    "@lgslabs/bits-pouch-crud": "^2.0.0",
    "@lgslabs/bits-scopes-util": "^1.0.0",
    "@lgslabs/child-process-promise": "^1.0.0",
    "graph-js": "^0.1.11",
    "semver": "^7.1.1"
  }
}
