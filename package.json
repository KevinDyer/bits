{
  "name": "bits",
  "version": "3.5.0",
  "description": "Bits Integrated Technology System",
  "author": "LGS Innovations",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/LGSInnovations/bits/issues"
  },
  "homepage": "https://github.com/LGSInnovations/bits#readme",
  "scripts": {
    "build": "yarn && npm run build:upgrade && npm run lint && npm run test && npm run autoclean",
    "build:upgrade": "cd support/scripts/upgrade-server && npm run build",
    "autoclean": "yarn autoclean --force",
    "bits:install": "yarn --prod && npm run autoclean",
    "lint": "eslint --fix --ext js ./",
    "test": "mocha --recursive",
    "clean": "rm -rf node_modules/ data/ support/scripts/upgrade-server/node_modules npm-error.log yarn-error.log",
    "dev": "HTTP_PORT=9000 HTTPS_PORT=9001 node app.js -v"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-html": "^4.0.5",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "@skidder/bits-auto-discovery": "^1.0.2",
    "@skidder/bits-crypto": "^1.0.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "digraph": "^1.4.1",
    "express": "^4.14.0",
    "graph-js": "^0.1.11",
    "helmet": "^3.1.0",
    "http-proxy": "^1.16.2",
    "level": "^1.6.0",
    "minimist": "^1.2.0",
    "morgan": "^1.7.0",
    "multer": "^1.2.1",
    "passport": "^0.3.2",
    "passport-http-bearer": "^1.0.1",
    "pem": "^1.9.4",
    "pouchdb": "^6.2.0",
    "pouchdb-find": "^6.2.0",
    "semver": "^5.3.0",
    "socket.io": "^1.7.1",
    "which": "^1.2.9",
    "winston": "^2.3.0"
  }
}
